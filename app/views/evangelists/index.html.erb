<section>
	<div class="wrap">
		<h1>Evangelists</h1>
		<table class="table table-bordered">
			<th>Name</th><th>Email</th><th>Phone</th><th>Wants to</th><th>Phone</th><th>Price</th>
<% @ev.each do |e| %>
<tr>
<td><%= e.fullname %></td> 
<td><%= e.email %></td> 
<td><%= e.phone %></td> 
<td><%= e.action %></td> 
<td><%= e.model %></td> 
<td><%= e.value %></td> 
</tr>
<% end %>
</table>
</div>
</section>
